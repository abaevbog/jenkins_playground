    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                } 
            }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }    
        }
        stage('New thing updated!') {
            steps {
                echo 'Woot Woot!'
            }    
        }
    } 
}